<h2>Most viewed authors</h2>
<table>
    <tbody>
        <tr>
            <%views.forEach(auth=>{%>
                <td>
                    <div><img src="<%=auth.aImage%>" height="200" width="282" alt="<%=auth.aname%>"></div>
                    <h3><a href="/authors/author?id=<%=auth.id%>"><%=auth.aname%></a></h3>
                    <h4>Views: <%=auth.s%></h4>
                </td>
            <%})%>
        </tr>
    </tbody>
</table>

